FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 22.4;

boundaryField
{
    wall
    {
        type            omegaWallFunction;
        value           uniform 22.4;
    }
    surfaces
    {
        type            omegaWallFunction;
        value           uniform 22.4;
    }
    main_inlet
    {
        type            slip;
    }
    main_outlet
    {
        type            slip;
    }
    branch_inlet
    {
        type            slip;
    }
    branch_outlet
    {
        type            slip;
    }
}


