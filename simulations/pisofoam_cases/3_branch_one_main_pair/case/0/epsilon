FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 0.00754;

boundaryField
{
    wall
    {
        type            epsilonWallFunction;
        value           uniform 0.00754;
    }
    surfaces
    {
        type            epsilonWallFunction;
        value           uniform 0.00754;
    }
    main_inlet
    {
        type            slip;
    }
    main_outlet
    {
        type            slip;
    }
    branch_inlet
    {
        type            slip;
    }
    branch_outlet
    {
        type            slip;
    }

}

// ************************************************************************* //
